<div class='container px-5 px-sm-0'>
<div class="row">
<div class="col-md-3">
</div>

<div class="col-md-6 ">
<h2>会員一覧</h2>
<table class='table table-hover table-inverse'>

<thead>
<tr>
<th>会員ID</th>
<th>氏名</th>
<th>メールアドレス</th>
<th>ステータス</th>
<th colspan="3"></th>
</tr>
</thead>
<tbody>
<% @customer.each do |customer| %>
<tr>
<td><%= customer.id %></td>
<td><%= link_to admin_customer_path(customer.id) do %>
<%= customer.last_name %><%= customer.first_name %>
<% end %>
</td>
<td><%= customer.email %></td>
<td><%= customer.is_deleted %></td>



</tr>

<% end %>
</tbody>
</table>

</div>
</div>
</div>
